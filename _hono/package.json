{
  "name": "hono-app",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --noCheck",
    "build:go": "GOOS=js GOARCH=wasm go build -gcflags=all='-N -l' -gcflags='-dwarflocationlists=true' -o ./src/bin/app.wasm ./main.go",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.0",
    "hono": "^4.9.6"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
